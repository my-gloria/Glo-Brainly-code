//Brainacs challenge 1
#include<stdio.h>

int main(){

printf (" Prime numbers btn 1-100 are: ");

int i, n, count;

for (n=1; n<=100; n++){
    count=0;

    for (i=1; i<=100;i++){
        if (n%i==0){
                count++;
    }
    }
    if (count==2){
    printf (" %d",n);
    }
}
    return 0;
}

